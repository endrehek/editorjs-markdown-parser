{
  "name": "editorjs-md-parser",
  "version": "0.0.4",
  "description": "Import from markdown and export editorjs data to markdown",
  "author": {
    "name": "Endre Heksum"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "type": "module",
  "main": "./dist/bundle.umd.js",
  "module": "./dist/bundle.js",
  "exports": {
    ".": {
      "import": "./dist/bundle.js",
      "require": "./dist/bundle.umd.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "watch": "vite serve"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.31.0"
  },
  "dependencies": {
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-squeeze-paragraphs": "^6.0.0",
    "require": "^2.4.0",
    "vite": "^6.3.5"
  }
}
